    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "prettier": "^3.3.3",
    "semantic-release": "^23.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.3",
    "vitest-axe": "^0.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
