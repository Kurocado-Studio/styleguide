    "@types/jest-axe": "^3.5.9",
    "@types/lodash-es": "^4.17.12",
    "@types/mdx": "^2.0.7",
    "@types/node": "20.5.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.2.21",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.2",
    "c8": "^10.1.2",
    "dotenv": "^16.4.5",
